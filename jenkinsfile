pipeline{
    agent {
        node {
            label "linux && java11"
        }
    }
    stages{
        stage('hello'){
            steps{
                echo 'pipeline'
            }
        }
    }

    post{
        always{
            echo "I will say Hello again"
        }
        success{
            echo "Huraa sukses"
        }
        failure{
            echo "shitt, its not good"
        }
        cleanup{
            echo "this is last time"
        }
    }
}